group MTK:
  plant automaton Verlichting:
    controllable c_aan;
    controllable c_uit;
    location uit:
      initial;
      marked;
      edge c_aan goto aan;
    location aan:
      edge c_uit goto uit;
  end
  plant automaton BedieningVerlichting:
    controllable c_auto;
    controllable c_hand;
    location auto:
      initial;
      marked;
      edge c_hand goto hand;
    location hand:
      edge c_auto goto auto;
  end
  plant automaton KnoppenVerlichting:
    monitor u_auto, u_hand, u_aan, u_uit;
    uncontrollable u_auto;
    uncontrollable u_hand;
    uncontrollable u_aan;
    uncontrollable u_uit;
    controllable c_auto_klaar;
    controllable c_hand_klaar;
    controllable c_aan_klaar;
    controllable c_uit_klaar;
    location los:
      initial;
      marked;
      edge u_auto goto auto;
      edge u_hand goto hand;
      edge u_aan goto aan;
      edge u_uit goto uit;
    location auto:
      edge c_auto_klaar when BedieningVerlichting.auto goto los;
    location hand:
      edge c_hand_klaar when BedieningVerlichting.hand goto los;
    location aan:
      edge c_aan_klaar when Verlichting.aan goto los;
    location uit:
      edge c_uit_klaar when Verlichting.uit goto los;
  end
  plant automaton Overdruk:
    controllable c_uit;
    controllable c_links;
    controllable c_rechts;
    location uit:
      initial;
      marked;
      edge c_links goto links;
      edge c_rechts goto rechts;
    location links:
      edge c_uit goto uit;
      edge c_rechts goto rechts;
    location rechts:
      edge c_uit goto uit;
      edge c_links goto links;
  end
  plant automaton BedieningOverdruk:
    controllable c_auto;
    controllable c_hand;
    location auto:
      initial;
      marked;
      edge c_hand goto hand;
    location hand:
      edge c_auto goto auto;
  end
  plant automaton KnoppenOverdruk:
    monitor u_auto, u_hand, u_links, u_rechts, u_uit;
    uncontrollable u_auto;
    uncontrollable u_hand;
    uncontrollable u_links;
    uncontrollable u_rechts;
    uncontrollable u_uit;
    controllable c_auto_klaar;
    controllable c_hand_klaar;
    controllable c_links_klaar;
    controllable c_rechts_klaar;
    controllable c_uit_klaar;
    location los:
      initial;
      marked;
      edge u_auto goto auto;
      edge u_hand goto hand;
      edge u_links goto links;
      edge u_rechts goto rechts;
      edge u_uit goto uit;
    location auto:
      edge c_auto_klaar when BedieningOverdruk.auto goto los;
    location hand:
      edge c_hand_klaar when BedieningOverdruk.hand goto los;
    location links:
      edge c_links_klaar when Overdruk.links goto los;
    location rechts:
      edge c_rechts_klaar when Overdruk.rechts goto los;
    location uit:
      edge c_uit_klaar when Overdruk.uit goto los;
  end
  plant automaton DVI:
    controllable c_uit;
    controllable c_oplopend;
    controllable c_aflopend;
    location uit:
      initial;
      marked;
      edge c_oplopend goto oplopend;
      edge c_aflopend goto aflopend;
    location oplopend:
      edge c_uit goto uit;
      edge c_aflopend goto aflopend;
    location aflopend:
      edge c_uit goto uit;
      edge c_oplopend goto oplopend;
  end
  plant automaton BedieningDVI:
    controllable c_auto;
    controllable c_hand;
    location auto:
      initial;
      marked;
      edge c_hand goto hand;
    location hand:
      edge c_auto goto auto;
  end
  plant automaton KnoppenDVI:
    monitor u_auto, u_hand, u_oplopend, u_aflopend, u_uit;
    uncontrollable u_auto;
    uncontrollable u_hand;
    uncontrollable u_oplopend;
    uncontrollable u_aflopend;
    uncontrollable u_uit;
    controllable c_auto_klaar;
    controllable c_hand_klaar;
    controllable c_oplopend_klaar;
    controllable c_aflopend_klaar;
    controllable c_uit_klaar;
    location los:
      initial;
      marked;
      edge u_auto goto auto;
      edge u_hand goto hand;
      edge u_oplopend goto oplopend;
      edge u_aflopend goto aflopend;
      edge u_uit goto uit;
    location auto:
      edge c_auto_klaar when BedieningOverdruk.auto goto los;
    location hand:
      edge c_hand_klaar when BedieningOverdruk.hand goto los;
    location oplopend:
      edge c_oplopend_klaar when DVI.oplopend goto los;
    location aflopend:
      edge c_aflopend_klaar when DVI.aflopend goto los;
    location uit:
      edge c_uit_klaar when Overdruk.uit goto los;
  end
  group Omroep:
    plant automaton Modus:
      controllable c_startlive;
      controllable c_stoplive;
      controllable c_startboodschap;
      controllable c_stopboodschap;
      location uit:
        initial;
        marked;
        edge c_startlive goto live;
        edge c_startboodschap goto boodschap;
      location live:
        edge c_stoplive goto uit;
        edge c_startboodschap goto boodschap;
      location boodschap:
        edge c_startlive goto live;
        edge c_stopboodschap goto uit;
    end
    plant automaton Knoppen:
      monitor u_startLive, u_stopLive, u_startBoodschap, u_stopBoodschap;
      uncontrollable u_startLive;
      uncontrollable u_stopLive;
      uncontrollable u_startBoodschap;
      uncontrollable u_stopBoodschap;
      controllable c_startLive_klaar;
      controllable c_stopLive_klaar;
      controllable c_startBoodschap_klaar;
      controllable c_stopBoodschap_klaar;
      location los:
        initial;
        marked;
        edge u_startLive goto startLive;
        edge u_stopLive goto stopLive;
        edge u_startBoodschap goto startBoodschap;
        edge u_stopBoodschap goto stopBoodschap;
      location startLive:
        edge c_startLive_klaar when Modus.live goto los;
      location stopLive:
        edge c_stopLive_klaar when Modus.uit goto los;
      location startBoodschap:
        edge c_startBoodschap_klaar when Modus.boodschap goto los;
      location stopBoodschap:
        edge c_stopBoodschap_klaar when Modus.uit goto los;
    end
  end
  requirement invariant BedieningVerlichting.c_hand needs KnoppenVerlichting.hand and not(VB1.SetAuto.setauto or VB2.SetAuto.setauto);
  requirement invariant BedieningVerlichting.c_auto needs KnoppenVerlichting.auto or (VB1.SetAuto.setauto or VB2.SetAuto.setauto);
  requirement invariant Verlichting.c_aan needs BedieningVerlichting.hand and KnoppenVerlichting.aan or BedieningVerlichting.auto and (VB1.Stand.calamiteit or VB2.Stand.calamiteit or (VB1.Stand.evacuatie or VB2.Stand.evacuatie));
  requirement invariant Verlichting.c_uit needs BedieningVerlichting.hand and KnoppenVerlichting.uit or BedieningVerlichting.auto and not(VB1.Stand.calamiteit or VB2.Stand.calamiteit or (VB1.Stand.evacuatie or VB2.Stand.evacuatie));
  requirement invariant BedieningOverdruk.c_hand needs KnoppenOverdruk.hand and not(VB1.SetAuto.setauto or VB2.SetAuto.setauto);
  requirement invariant BedieningOverdruk.c_auto needs KnoppenOverdruk.auto or (VB1.SetAuto.setauto or VB2.SetAuto.setauto);
  requirement invariant Overdruk.c_uit needs BedieningOverdruk.hand and KnoppenOverdruk.uit or BedieningOverdruk.auto and not(VB1.Stand.calamiteit or VB2.Stand.calamiteit or (VB1.Stand.evacuatie or VB2.Stand.evacuatie));
  requirement invariant Overdruk.c_links needs BedieningOverdruk.hand and KnoppenOverdruk.links or BedieningOverdruk.auto and ((VB1.Stand.evacuatie or VB1.Stand.calamiteit) and not(VB2.Stand.evacuatie or VB2.Stand.calamiteit));
  requirement invariant Overdruk.c_rechts needs BedieningOverdruk.hand and KnoppenOverdruk.rechts or BedieningOverdruk.auto and ((VB2.Stand.evacuatie or VB2.Stand.calamiteit) and not(VB1.Stand.evacuatie or VB1.Stand.calamiteit));
  requirement invariant BedieningDVI.c_hand needs KnoppenDVI.hand and not(VB1.SetAuto.setauto or VB2.SetAuto.setauto);
  requirement invariant BedieningDVI.c_auto needs KnoppenDVI.auto or (VB1.SetAuto.setauto or VB2.SetAuto.setauto);
  requirement invariant DVI.c_uit needs BedieningDVI.hand and KnoppenDVI.uit or BedieningDVI.auto and not(VB1.Stand.calamiteit or VB2.Stand.calamiteit or (VB1.Stand.evacuatie or VB2.Stand.evacuatie));
  requirement invariant DVI.c_aflopend needs BedieningDVI.hand and KnoppenDVI.aflopend or BedieningDVI.auto and ((VB2.Stand.evacuatie or VB2.Stand.calamiteit) and not(VB1.Stand.evacuatie or VB1.Stand.calamiteit));
  requirement invariant DVI.c_oplopend needs BedieningDVI.hand and KnoppenDVI.oplopend or BedieningDVI.auto and ((VB1.Stand.evacuatie or VB1.Stand.calamiteit) and not(VB2.Stand.evacuatie or VB2.Stand.calamiteit));
end
