	group GUI_Systemenlijst:

    plant dummy:
      event e_dummy;
      location:
        initial; marked;
        edge e_dummy;
    end
    plant TabbladVB:
      event e_Buis1, e_Buis2;
      monitor;
      location B1:
        initial;
        edge e_Buis2 goto B2;
      location B2:
        edge e_Buis1 goto B1;
    end
    svgin id "TabbladVerkeersbuis1" event TabbladVB.e_Buis1;
    svgout id "TabbladVerkeersbuis1Achtergrond" attr "stroke" value if TabbladVB.B1: "white" else "black" end;
    svgin id "TabbladVerkeersbuis2" event TabbladVB.e_Buis2;
    svgout id "TabbladVerkeersbuis2Achtergrond" attr "stroke" value if TabbladVB.B2: "white" else "black" end;

    group Ventilatie:
      svgin id "SystemenVentilatieHandKnop" event if TabbladVB.B1: VB1.Ventilatie.Knoppen.u_hand else VB2.Ventilatie.Knoppen.u_hand end;
      svgout id "SystemenVentilatieHand" attr "fill" value if (VB1.Ventilatie.Bediening.hand and TabbladVB.B1) or (VB2.Ventilatie.Bediening.hand and TabbladVB.B2): "gold" else "grey" end;
      svgin id "SystemenVentilatieAutoKnop" event if TabbladVB.B1: VB1.Ventilatie.Knoppen.u_auto else VB2.Ventilatie.Knoppen.u_auto end;
      svgout id "SystemenVentilatieAuto" attr "fill" value if (VB1.Ventilatie.Bediening.auto and TabbladVB.B1) or (VB2.Ventilatie.Bediening.auto and TabbladVB.B2): "gold" else "grey" end;

      svgin id "SystemenVentilatieRichtingTegenKnop" event if TabbladVB.B1: VB1.Ventilatie.Knoppen.u_tegenRijrichting else VB2.Ventilatie.Knoppen.u_tegenRijrichting end;
      svgout id "SystemenVentilatieRichtingTegen" attr "fill" value if (VB1.Ventilatie.Richting.tegenRijrichting and TabbladVB.B1) or (VB2.Ventilatie.Richting.tegenRijrichting and TabbladVB.B2): "gold" else "grey" end;
