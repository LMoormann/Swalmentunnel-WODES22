import "../Supervisor/Supervisor.cif", "Dropdown.cif", "GUI_Timer.cif";

group GUI:
  svgfile "GUI.svg";

  group GUI_Overzichtsplattegrond:
    group StandVerkeersbuis1:
      svgout id "OverzichtsplattegrondVerkeersbuis1Stand" attr "fill" value if VB1.Stand.operationeel: "grey" elif VB1.Stand.standby: "#ffcc00" elif VB1.Stand.calamiteit: "#d26900" elif VB1.Stand.evacuatie: "#d26900" elif VB1.Stand.herstel: "#66cc99" elif VB1.Stand.ondersteunend: "#d09556" elif VB1.Stand.onderhoud: "#339966" else "grey" end;
      svgout id "OverzichtsplattegrondVerkeersbuis1StandDriehoek" attr "fill" value if VB1.Stand.operationeel: "grey" elif VB1.Stand.standby: "#ffcc00" elif VB1.Stand.calamiteit: "#d26900" elif VB1.Stand.evacuatie: "#d26900" elif VB1.Stand.herstel: "#66cc99" elif VB1.Stand.ondersteunend: "#d09556" elif VB1.Stand.onderhoud: "#339966" else "grey" end;
    end
    group Afsluitboom1:
      svgout id "OverzichtsplattegrondAfsluitboomDicht1" attr "opacity" value if VB1.AB.Stand.neer: "1" elif VB1.AB.Beweging.neer: ".5" else "0" end;
      svgout id "OverzichtsplattegrondAfsluitboomOpen1" attr "opacity" value if VB1.AB.Stand.op: "1" else "0" end;
    end
    group Verkeerslicht1:
      svgout id "OverzichtsplattegrondVerkeerslichtAchtergrond1" attr "opacity" value if VB1.VRI.Stand.uit: ".5" else "1" end;
      svgout id "OverzichtsplattegrondVerkeerslicht1CircleRed" attr "opacity" value if VB1.VRI.Stand.rood: "1" else "0" end;
      svgout id "OverzichtsplattegrondVerkeerslicht1CircleYellow" attr "opacity" value if VB1.VRI.Stand.geel or VB1.VRI.Stand.knipper: "1" else "0" end;
    end
    group StandVerkeersbuis2:
      svgout id "OverzichtsplattegrondVerkeersbuis2Stand" attr "fill" value if VB2.Stand.operationeel: "grey" elif VB2.Stand.standby: "#ffcc00" elif VB2.Stand.calamiteit: "#d26900" elif VB2.Stand.evacuatie: "#d26900" elif VB2.Stand.herstel: "#66cc99" elif VB2.Stand.ondersteunend: "#d09556" elif VB2.Stand.onderhoud: "#339966" else "grey" end;
      svgout id "OverzichtsplattegrondVerkeersbuis2StandDriehoek" attr "fill" value if VB2.Stand.operationeel: "grey" elif VB2.Stand.standby: "#ffcc00" elif VB2.Stand.calamiteit: "#d26900" elif VB2.Stand.evacuatie: "#d26900" elif VB2.Stand.herstel: "#66cc99" elif VB2.Stand.ondersteunend: "#d09556" elif VB2.Stand.onderhoud: "#339966" else "grey" end;
    end
    group Afsluitboom2:
      svgout id "OverzichtsplattegrondAfsluitboomDicht2" attr "opacity" value if VB2.AB.Stand.neer: "1" elif VB2.AB.Beweging.neer: ".5" else "0" end;
      svgout id "OverzichtsplattegrondAfsluitboomOpen2" attr "opacity" value if VB2.AB.Stand.op: "1" else "0" end;
    end
    group Verkeerslicht2:
      svgout id "OverzichtsplattegrondVerkeerslichtAchtergrond2" attr "opacity" value if VB2.VRI.Stand.uit: ".5" else "1" end;
      svgout id "OverzichtsplattegrondVerkeerslicht2CircleRed" attr "opacity" value if VB2.VRI.Stand.rood: "1" else "0" end;
      svgout id "OverzichtsplattegrondVerkeerslicht2CircleYellow" attr "opacity" value if VB2.VRI.Stand.geel or VB2.VRI.Stand.knipper: "1" else "0" end;
    end
    group StandMiddentunnelkanaal:
      svgout id "OverzichtsplattegrondMiddentunnelkanaal1Achtergrond" attr "fill" value if VB1.Stand.operationeel and VB2.Stand.operationeel: "grey" elif VB1.Stand.calamiteit or VB2.Stand.calamiteit: "#ffcc00" elif VB1.Stand.evacuatie or VB2.Stand.evacuatie: "#d26900" elif VB1.Stand.onderhoud and VB2.Stand.onderhoud: "#d09556" else "grey" end;
    end
  end

