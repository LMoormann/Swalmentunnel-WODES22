//       svgout id "SystemenlijstHF" attr "visibility" value if (Dropdown.open and GUI_Systemenlijst.TabbladVB.B1 and VB1.OmroepHFStand.HF) or (Dropdown.open and GUI_Systemenlijst.TabbladVB.B2 and VB2.OmroepHFStand.HF): "visible" else "hidden" end;
